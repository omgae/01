local function soul()
       for i,v in pairs(game:GetService("Workspace").Soul:GetDescendants()) do
            if v:IsA"TouchTransmitter" then
                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 0)
                firetouchinterest(game.Players.LocalPlayer.Character.HumanoidRootPart, v.Parent, 1)
            end
        end 
end
local function Invisble()
        for i,v in pairs(game:GetService("Workspace").pantaeieii:GetDescendants()) do
           if v.Name == "Weld" then
               v:Destroy()
               end
       end
end
