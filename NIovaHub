--Whatever your reason for looking here, here are all the games my script works with.

if game.PlaceId == 6913960180 then --Undertale
    loadstring(game:HttpGet("https://raw.githubusercontent.com/omgae/01/main/Undertale"))()
elseif game.PlaceId == 221718525 then--NinjaTycoon
    loadstring(game:HttpGet("https://raw.githubusercontent.com/omgae/01/main/NinjaTycoon"))()
elseif game.PlaceId == 11448431235 then--RockFruit
    loadstring(game:HttpGet("https://raw.githubusercontent.com/omgae/01/main/RockFruit"))()
    end
